{
  "expo": {
    "name": "My Wallet",
    "slug": "my-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/new/blue/splash-icon-rupee.png",
    "scheme": "mywallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/new/blue/adaptive-icon-rupee.png",
        "backgroundImage": "./src/assets/images/custom/android-icon-background.png",
        "monochromeImage": "./src/assets/images/custom/icon-rupee.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.satish.mywallet"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/new/blue/splash-icon-rupee.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#007AFF", //"#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      [
        "./src/plugins/withAndroidSplits",
        {
          "abis": [
            "arm64-v8a"
          ],
          "universalApk": false
        }
      ],
      "./src/plugins/withAndroidBuildOptimization",
      "./src/plugins/withProguardRules",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}